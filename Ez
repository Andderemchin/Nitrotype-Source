                r / 1e3
            }
            ,
            t.getSpeed = function() {
                if (!t.state.racers || !t.state.racers.length)
                    return 0;
                var e = t.state.racers.find((function(e) {
                    return e.userID === t.props.user.userID
                }
                ));
                if (!e)
                    return 0;
                var n = 0;
                if (e.progress.completeStamp) {
                    var r = t.getRaceDuration(e.progress.startStamp, e.progress.completeStamp);
                    n = Object(p.d)(e.progress.typed - e.progress.skipped, r)
                } else
                    e.progress.typed && (n = (n = Math.floor(Object(p.d)(e.progress.typed - e.progress.skipped, t.secs / 1e3))) > 250 || n < 0 ? 0 : n);
                return n
            }
            ,
            t.sendFpsToServer = function() {
                var e, n = arguments.length > 0 && void 0 !== arguments[0] && arguments[0], r = arguments.length > 1 ? arguments[1] : void 0, a = arguments.length > 2 ? arguments[2] : void 0, o = arguments.length > 3 ? arguments[3] : void 0, i = arguments.length > 4 ? arguments[4] : void 0, s = null === (e = t.setupData) || void 0 === e ? void 0 : e.id, c = !!s, l = "track" === (null == i ? void 0 : i.source);
                if (l && (l || c) && !t.hasHandledSendFpsRequest) {
                    t.hasHandledSendFpsRequest = !0,
                    l && (r = "".concat(i.type, " error: ").concat(i.status));
                    var u = t.game.getFpsCache()
                      , f = t.props.cacheId
                      , d = Math.floor(performance.now() / 1e3)
                      , m = t.game.getRendererQuality()
                      , h = t.game.getEffectsLevel()
                      , y = t.game.getDidLoseWebGLContext()
                      , g = t.game.getRendererType()
                      , v = t.game.getDynamicPerformanceSummary()
                      , b = jn({
                        adNetwork: window.__adNetwork || "unknown",
                        raceID: s,
                        release: f,
                        quality: m,
                        effects: h,
                        webGLContextLost: y,
                        renderer: g,
                        timeOnPage: d,
                        trackLeaderUserID: t.props.trackLeaderUserID,
                        position: t.getRacePlace(),
                        racesPlayed: t.props.user.racesPlayed,
                        dq: n ? 1 : 0,
                        isQualifying: t.state.qualifyingRace ? 1 : 0,
                        webglSupport: Object(p.C)() ? 1 : 0,
                        errorMessage: r,
                        errorStack: a,
                        error: o
                    }, u);
                    v && (b = jn(jn({}, b), v)),
                    Object(p.b)("/api/v2/race/perf-log", {
                        method: "post",
                        params: b
                    }, (function() {
                        return null
                    }
                    )).then((function() {
                        return null
                    }
                    ), (function() {
                        return null
                    }
                    ))
                }
            }
            ,
            t.getAccuracy = function() {
                var e = arguments.length > 0 && void 0 !== arguments[0] ? arguments[0] : 100;
                if (!t.state.racers || !t.state.racers.length)
                    return 100;
                var n, r = t.state.racers.find((function(e) {
                    return e.userID === t.props.user.userID
                }
                ));
                return r ? ((n = r.progress.completeStamp ? Object(p.c)(r.progress.typed - r.progress.skipped, r.progress.errors) : Math.round(Object(p.c)(r.progress.typed - r.progress.skipped, r.progress.errors)) || e) < 0 && (n = 0),
                n) : 100
            }
            ,
            t.getFriendsCount = function() {
                var e = (t.props.user.friends || []).map((function(e) {
                    return e.userID
                }
                ));
                return t.state.racers.filter((function(t) {
                    return e.includes(t.userID)
                }
                )).length
            }
            ,
