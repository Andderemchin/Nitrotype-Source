            }
            ,
            t.racerComplete = function() {
                var e, n = t.state.racers.find((function(e) {
                    return e.userID === t.props.user.userID
                }
                ));
                (t.finishedTyping(),
                t.props.setRaceStatus("complete"),
                t.state.qualifyingRace) && t.setState({
                    playerCarID: null === (e = n.profile) || void 0 === e ? void 0 : e.carID
                });
                if (t.sendFpsToServer(),
                t.playerUpdatesInterval && clearInterval(t.playerUpdatesInterval),
                t.state.qualifyingRace) {
                    var r = t.getSpeed()
                      , a = t.getAccuracy()
                      , o = t.props.user.raceSounds;
                    t.props.loggedIn ? t.props.setQualifyingInfo(r, a) : t.props.setGuestQualifyingInfo({
                        speed: r,
                        accuracy: a,
                        raceSounds: o
                    }),
                    t.setState({
                        showingQualifyingModal: !0
                    })
                } else
                    setTimeout((function() {
                        t.showRaceResults()
                    }
                    ), 1600),
                    t.save(n);
                t.gtmRaceEvent("complete")
            }
            ,
            t.showRaceResults = function() {
