            t.showRaceError = function() {
                var e = arguments.length > 0 && void 0 !== arguments[0] ? arguments[0] : {};
                t.stopCapturingRaceInput();
                var n = t.state.racers.find((function(e) {
                    return e.userID === t.props.user.userID
                }
                ));
                if (n && (!n.disqualified && n.progress.finished || n.disqualified || n.error))
